		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.4",
		"@types/lodash": "^4.17.16",
		"@types/node": "^22.15.3",
		"daisyui": "^5.0.35",
		"eslint": "^9.25.1",
